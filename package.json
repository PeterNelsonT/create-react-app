{
  "name": "react-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "node --max-old-space-size=4096 scripts/start.js local",
    "start:dev": "node scripts/start.js dev",
    "build:dev": "node scripts/build.js dev",
    "start:qa": "node scripts/start.js qa",
    "build:qa": "node scripts/build.js qa",
    "start:prod": "node scripts/start.js prod",
    "build:prod": "node scripts/build.js prod",
    "dev-vite": "vite",
    "start:dev-vite": "vite --mode dev",
    "build:dev-vite": "tsc && vite build --mode dev",
    "start:qa-vite": "vite --mode qa",
    "build:qa-vite": "tsc && vite build --mode qa",
    "start:prod-vite": "vite --mode prod",
    "build:prod-vite": "tsc && vite build --mode prod",
    "preview": "vite preview",
    "commit": "git-cz",
    "lint": "lint-staged --config .lintstagedrc.json",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "lint:css": "stylelint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "tscheck": "tsc -w"
  },
